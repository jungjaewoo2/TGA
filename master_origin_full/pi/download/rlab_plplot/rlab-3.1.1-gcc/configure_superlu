#define HAVE_SUITESPARSE 1
EOF
    SPARSE_LIB="$SPARSE_LIB -lumfpack -lcholmod -lbtf -lamd -lcamd -lccolamd -lcolamd -lcsparse -lcxsparse \
-lklu -lldl -lrbio -lspqr -lsuitesparseconfig"
    echo "$ac_t""yes (${SPARSE_LIB})" 1>&6
  else
    echo "configure: failed program was:" >&5
    cat conftest.$ac_ext >&5
    rm -rf conftest*
    echo "$ac_t""no" 1>&6; LIBS="$save_libs"
  fi
  LIBS="$save_libs"
  rm -f conftest*
fi

