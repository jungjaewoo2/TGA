LIBS="$ac_save_LIBS"

fi
if eval "test \"`echo '$ac_cv_lib_'$ac_lib_var`\" = yes"; then
  echo "$ac_t""yes" 1>&6
  X11LIB="$X11LIB -lsocket -lnsl"
else
  echo "$ac_t""no" 1>&6
fi


fi

#
# Check whether --with-pgplot or --without-pgplot was given.
#
/bin/rm -rf r_pgplot.o 2>/dev/null
with_pgplot="${with_pgplot}${enable_pgplot}"
if [ -n "${with_pgplot}" ]; then
  with_pgplot=pgplot
  if test -n "$PLOT_PROG" ; then
    echo "ERROR! cannot specify more than one plot interface!"
    exit 1
  fi
  if ! test -s "flibs/pgplot/src/copyright.notice" ; then
    echo "pgplot: ERROR: You tried to specify pgplot interface, but the directory"
    echo "pgplot: ERROR:    flibs/pgplot/src   does not exist, or is empty."
    echo "pgplot: ERROR: Because of license restrictions, it is user (your)"
    echo "pgplot: ERROR: responsability to obtain pgplot source code and"
    echo "pgplot: ERROR: untar it to   flibs/pgplot/src   directory."
    echo "pgplot: ERROR: Please se   flibs/pgplot/install.txt   for detailed instructions."
    exit 1
  fi
  with_plplot="no"
  echo "// rlabplus // PGPLOT is enabled through $with_pgplot"
  PLOT_PROG="pgplot"
  PLIBS="-L$with_pgplot -lpgplot $X11LIB"
  PLIB_INC_DIR="-I$with_pgplot"
  PLOT_DIR="$with_pgplot"
  PLIBS_A="${with_pgplot}/libpgplot.a ${with_pgplot}/libcpgplot.a"
  RLAB_PLIB_C="r_pgplot.c"
  RLAB_PLIB_O="r_pgplot.o"
  PGPLOT="PGPLOT"
  cat >> confdefs.h <<\EOF
#define HAVE_RLAB_PGPLOT 1
EOF
fi

#
# plplot libraries comes in two variants, with and without d at the end
#
#
if [ "$with_plplot" = "yes" ]; then
  rm r_plot.o 2>/dev/null 1>&2
  echo $ac_n "checking for -lplplotd""... $ac_c" 1>&6
  echo "configure:3708: checking for -lplplotd" >&5
  save_libs="$LIBS"; LIBS="-lplplotd -lplplotcxxd $CMDLIBS $LIBS"
  cat > conftest.$ac_ext <<EOF
#include "confdefs.h"
int MAIN__(){return(1);}
int main() {
return(1);
; return 0; }
EOF
  if { (eval echo configure \"$ac_link\") 1>&5; (eval $ac_link) 2>&5; } && test -s conftest${ac_exeext}; then
    rm -rf conftest*
    echo "$ac_t""yes" 1>&6; cat >> confdefs.h <<\EOF
#define HAVE_RLAB_PLPLOT 1
EOF
    PLOT_PROG="plplot"
    PLIBS="-lplplotd -lplplotcxxd -lX11"
    RLAB_PLIB_C="r_plplot.c"
    RLAB_PLIB_O="r_plplot.o"
    [ -d /usr/include/plplot ]&& PLIB_INC_DIR="-I/usr/include/plplot"
    [ -d /usr/local/include/plplot ]&& PLIB_INC_DIR="${PLIB_INC_DIR} -I/usr/local/include/plplot"
  else
#   # try two:
    LIBS="$save_libs"
    echo $ac_n "no let's try checking for -lplplot""... $ac_c" 1>&6
    echo "configure:3708: checking for -lplplot" >&5
    save_libs="$LIBS"; LIBS="-lplplot -lplplotcxx $CMDLIBS $LIBS"
    cat > conftest.$ac_ext <<EOF
#include "confdefs.h"
int MAIN__(){return(1);}
int main() {
return(1);
; return 0; }
EOF
    if { (eval echo configure \"$ac_link\") 1>&5; (eval $ac_link) 2>&5; } && test -s conftest${ac_exeext}; then
      rm -rf conftest*
      echo "$ac_t""yes" 1>&6; cat >> confdefs.h <<\EOF
#define HAVE_RLAB_PLPLOT 1
EOF
      PLOT_PROG="plplot"
      PLIBS="-lplplot -lplplotcxx -lX11"
      RLAB_PLIB_C="r_plplot.c"
      RLAB_PLIB_O="r_plplot.o"
      [ -d /usr/include/plplot ]&& PLIB_INC_DIR="-I/usr/include/plplot"
      [ -d /usr/local/include/plplot ]&& PLIB_INC_DIR="${PLIB_INC_DIR} -I/usr/local/include/plplot"
    else
      echo "configure: failed program was:" >&5
      cat conftest.$ac_ext >&5
      rm -rf conftest*
      echo "$ac_t""no" 1>&6; LIBS="$save_libs"
    fi
  fi
  rm -f conftest*
fi
LIBS="$save_libs"

echo "// rlabplus //"
echo "// rlabplus // Mandatory libraries section: start";
echo "// rlabplus //"

