#
# IMAGEMAGIC IS MAKING ITS WAY TO RLAB:
#
/bin/rm -rf r_imagemagick.o 2>/dev/null
with_im="$with_im$enable_im"
if [ "$with_im" = "yes" ]; then
  echo $ac_n "checking for -lMagickWand""... $ac_c" 1>&6
  wandconfig=`which Wand-config 2>/dev/null`
  [ -z "${wandconfig}" ]&&   (wandconfig=`which MagickWand-config 2>/dev/null`)
  if [ -n "${wandconfig}" ]; then
    IMAGEMAGICKLIB="`${wandconfig} --libs | grep MagickWand-6`"
    if [ -n "${IMAGEMAGICKLIB}" ]; then
      RIMAGEMAGICK_C="r_imagemagick.c"
      RIMAGEMAGICK_O="r_imagemagick.o"
      IMCFLAGS="`${wandconfig} --cflags`"
      echo "$ac_t""yes (${IMAGEMAGICKLIB})" 1>&6
      cat >> confdefs.h <<\EOF
#define HAVE_IMAGEMAGICK 1
EOF
    fi
  fi
fi

#
# LIBGPHOTO2 IS HERE:
#
/bin/rm -rf rgphoto2.o 2>/dev/null
with_gphoto2="${with_gphoto2}${enable_gphoto2}"
if [ "$with_gphoto2" = "yes" ]; then
  echo $ac_n "checking for -lgphoto2""... $ac_c" 1>&6
  if [ -x `which gphoto2-config` ]; then
    GPHOTO2LIB="`gphoto2-config --libs`"
    RGPHOTO2_C="rgphoto2.c"
    RGPHOTO2_O="rgphoto2.o"
    G2FLAGS="`gphoto2-config --cflags`"
    echo "$ac_t""yes (${GPHOTO2LIB})" 1>&6;
    cat >> confdefs.h <<\EOF
#define HAVE_GPHOTO2 1
EOF
  fi
fi

#
# PYTHON IS HERE:
#
/bin/rm -rf rpython.c 2>/dev/null
/bin/rm -rf rpython.o 2>/dev/null
with_python="${with_python}${enable_python}"
if [ "$with_python" = "yes" ]; then
  PYTHON_VER=`python --version 2>&1 | gawk  '{
    split($2, vv, ".");
    print vv[1] "." vv[2];
  }'`
# # the library is linked to a particular python version
  [ -z "$PYTHON_VER" ]|| PYTHON_LIB="-lpython$PYTHON_VER"
  echo $ac_n "checking for ${PYTHON_LIB}""... $ac_c" 1>&6
  echo "configure:3708: checking for ${PYTHON_LIB}" >&5
  save_libs="$LIBS"; LIBS="${PYTHON_LIB} $CMDLIBS $LIBS"
  cat > conftest.$ac_ext <<EOF
#line 3711 "configure"
#include "confdefs.h"
#include "python${PYTHON_VER}/Python.h"
int MAIN__(){return(1);}
int main() {
return(1);
; return 0; }
EOF
  if { (eval echo configure:3718: \"$ac_link\") 1>&5; (eval $ac_link) 2>&5; } && test -s conftest${ac_exeext}; then
    rm -rf conftest*
    echo "$ac_t""yes" 1>&6; cat >> confdefs.h <<\EOF
#define HAVE_PYTHON 1
EOF
    [ -z "$PYTHON_VER" ]|| sed s%@PYTHON_LIB@%$PYTHON_VER%g rpython.c.in > rpython.c
    RPYTHON_C="rpython.c"
    RPYTHON_O="rpython.o"
  else
    echo "configure: failed program was:" >&5
    cat conftest.$ac_ext >&5
    rm -rf conftest*
    echo "$ac_t""no" 1>&6; LIBS="$save_libs"
  fi
  LIBS="$save_libs"
  rm -f conftest*
fi

#
# JAVA IS HERE:
#
/bin/rm -rf rjava.o 2>/dev/null
with_jvm="${with_jvm}${enable_jvm}"
if [ "$with_jvm" = "yes" ]; then
  with_jvm="/usr/lib${CPUINFO}"
fi
if [ "${with_jvm}" = "no" ]|| [ "${with_jvm}" = "" ]; then
  echo "Linking with JVM disabled!"
else
  if ! test -s "${with_jvm}/libjvm.so" ; then
    echo "Selected Java Virtual Machine does not exist! Consider building rlab without it."
    exit 1
  fi
  JVMLIB="-L${with_jvm} -ljvm"
  LIBJVM_DIR="${with_jvm}"
  RJAVA_C="rjava.c"
  RJAVA_O="rjava.o"
  echo "// rlabplus // Java Virtual Machine '${with_jvm}/libjvm.so' enabled."
  cat >> confdefs.h <<\EOF
#define HAVE_JVM 1
EOF
fi

echo "// rlabplus //"
echo "// rlabplus // Optional libraries section: end"
echo "// rlabplus //"


# Check whether --with-plotmtv or --without-plotmtv was given.
if test "${with_plotmtv+set}" = set; then
  withval="$with_plotmtv"

  if test -n "$PLOT_PROG" ; then
    echo "ERROR! cannot specify more than one plot interface!"
  fi
  PLIBS=""
  PLIB_INC_DIR=""
  PLOT_PROG="plotmtv"

fi



if test "$PLOT_PROG" = "plotmtv" ; then
   # Extract the first word of "plotmtv", so it can be a program name with args.
set dummy plotmtv; ac_word=$2
echo $ac_n "checking for $ac_word""... $ac_c" 1>&6
echo "configure:4148: checking for $ac_word" >&5
if eval "test \"`echo '$''{'ac_cv_prog_PLOT_PROG'+set}'`\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  if test -n "$PLOT_PROG"; then
  ac_cv_prog_PLOT_PROG="$PLOT_PROG" # Let the user override the test.
else
  IFS="${IFS= 	}"; ac_save_ifs="$IFS"; IFS=":"
  ac_dummy="$PATH"
  for ac_dir in $ac_dummy; do
    test -z "$ac_dir" && ac_dir=.
    if test -f $ac_dir/$ac_word; then
      ac_cv_prog_PLOT_PROG="plotmtv"
      break
    fi
  done
  IFS="$ac_save_ifs"
  test -z "$ac_cv_prog_PLOT_PROG" && ac_cv_prog_PLOT_PROG="nothing"
fi
fi
PLOT_PROG="$ac_cv_prog_PLOT_PROG"
if test -n "$PLOT_PROG"; then
  echo "$ac_t""$PLOT_PROG" 1>&6
else
  echo "$ac_t""no" 1>&6
fi

fi



# Check whether --with-pgraf or --without-pgraf was given.
if test "${with_pgraf+set}" = set; then
  withval="$with_pgraf"

  if test -n "$PLOT_PROG" ; then
    echo "ERROR! cannot specify more than one plot interface!"
  fi
  PLIBS=""
  PLIB_INC_DIR=""
  PLOT_PROG="pgraf"

fi



if test "$PLOT_PROG" = "pgraf" ; then
   # Extract the first word of "pgraf", so it can be a program name with args.
set dummy pgraf; ac_word=$2
echo $ac_n "checking for $ac_word""... $ac_c" 1>&6
echo "configure:4198: checking for $ac_word" >&5
if eval "test \"`echo '$''{'ac_cv_prog_PLOT_PROG'+set}'`\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  if test -n "$PLOT_PROG"; then
  ac_cv_prog_PLOT_PROG="$PLOT_PROG" # Let the user override the test.
else
  IFS="${IFS= 	}"; ac_save_ifs="$IFS"; IFS=":"
  ac_dummy="$PATH"
  for ac_dir in $ac_dummy; do
    test -z "$ac_dir" && ac_dir=.
    if test -f $ac_dir/$ac_word; then
      ac_cv_prog_PLOT_PROG="pgraf"
      break
    fi
  done
  IFS="$ac_save_ifs"
  test -z "$ac_cv_prog_PLOT_PROG" && ac_cv_prog_PLOT_PROG="nothing"
fi
fi
PLOT_PROG="$ac_cv_prog_PLOT_PROG"
if test -n "$PLOT_PROG"; then
  echo "$ac_t""$PLOT_PROG" 1>&6
else
  echo "$ac_t""no" 1>&6
fi

fi







# Check whether --enable-GC or --disable-GC was given.
if test "${enable_GC+set}" = set; then
  enableval="$enable_GC"
  if test "$enableval" = "yes" ; then
               cat >> confdefs.h <<\EOF
#define HAVE_GC 1
EOF
LIBGC="../gc/.libs/libgc.a";
                                   IGC="-I./gc/include"
             fi
else
  cat >> confdefs.h <<\EOF
#define HAVE_GC 1
EOF
 LIBGC="./gc/.libs/libgc.a";
                                 IGC="-I./gc/include"
fi


cat > conftest.$ac_ext <<EOF
#line 4275 "configure"
#include "confdefs.h"
#include <stdio.h>
#include <stdlib.h>
int main() {
int (*x)()=(int(*)())fprintf;
; return 0; }
EOF
if { (eval echo configure:4282: \"$ac_link\") 1>&5; (eval $ac_link) 2>&5; } && test -s conftest${ac_exeext}; then
  rm -rf conftest*
  cat >> confdefs.h <<\EOF
#define HAVE_FPRINTF_DEC 1
EOF

else
  echo "configure: failed program was:" >&5
  cat conftest.$ac_ext >&5
fi
rm -f conftest*
cat > conftest.$ac_ext <<EOF
#line 4294 "configure"
#include "confdefs.h"
#include <stdio.h>
#include <stdlib.h>
int main() {
int (*x)()=(int(*)())sprintf;
; return 0; }
EOF
if { (eval echo configure:4301: \"$ac_link\") 1>&5; (eval $ac_link) 2>&5; } && test -s conftest${ac_exeext}; then
  rm -rf conftest*
  cat >> confdefs.h <<\EOF
#define HAVE_SPRINTF_DEC 1
EOF

else
  echo "configure: failed program was:" >&5
  cat conftest.$ac_ext >&5
fi
rm -f conftest*
cat > conftest.$ac_ext <<EOF
#line 4313 "configure"
#include "confdefs.h"
#include <stdio.h>
#include <stdlib.h>
int main() {
int (*x)()=(int(*)())fread;
; return 0; }
EOF
if { (eval echo configure:4320: \"$ac_link\") 1>&5; (eval $ac_link) 2>&5; } && test -s conftest${ac_exeext}; then
  rm -rf conftest*
  cat >> confdefs.h <<\EOF
#define HAVE_FREAD_DEC 1
EOF

else
  echo "configure: failed program was:" >&5
  cat conftest.$ac_ext >&5
