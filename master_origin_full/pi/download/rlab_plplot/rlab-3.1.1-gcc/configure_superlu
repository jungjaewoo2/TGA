      fi
    fi
    YACC="$ac_cv_prog_YACC"
    if test -n "$YACC"; then
      echo "$ac_t""$YACC" 1>&6
    else
      echo "$ac_t""no" 1>&6
    fi

    test -n "$YACC" && break
  done
#   test -n "$YACC" || YACC="yacc"
  YFLAGS="-vdtl"
fi


