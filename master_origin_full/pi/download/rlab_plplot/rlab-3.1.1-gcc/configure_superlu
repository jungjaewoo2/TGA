# Check whether --with-cc or --without-cc was given.
if test "${with_cc+set}" = set; then
  withval="$with_cc"
  CC="cc"
fi

# Check whether --with-gcc or --without-gcc was given.
if test "${with_gcc+set}" = set; then
  withval="$with_gcc"
  CC="gcc" GCC=1
fi



#
# On some systems it is better to use something other than cc
# IFF it is delivered with UNIX (not an extra)
# Also, we may need to set C_SH_FLAGS, etc... for dynamic
# linking, and Fortran, etc...
#
# The following switch statement allows us to set certain make
# variables that cannot be determined with configure.
#

    case "$system" in
	aix*|AIX*|rs*|RS*|ibm*|IBM* )
            case "$system" in
               *3.* )
 	          F77LIBS="-L. -lxlf -lxlfutil"
	          F77CONV="-DUSE_LOWER"
	          CC="cc"
	          CFLAGS="-qlanglvl=ansi"
                  FFLAGS="-O"
                  ;;
               *4.* )
 	          F77LIBS="-L. -lxlf -lxlfutil -lxlf90"
	          F77CONV="-DUSE_LOWER"
	          CC="cc"
	          CFLAGS="-O"
                  FFLAGS="-O"
                  ;;
               esac
	;;
	alpha*|ALPHA*|Alpha*|OSF* )
	    CC="cc -std1 -Dunix"
	    CFLAGS="-g"
            FFLAGS="-g"
	    F77LIBS="-L. -lfor -lUfor -lFutil -lots -lm -lc"
	    F77CONV="-DUSE_F2C"
	;;
	convex*|ConvexOS* )
	;;
	dg*|DG* )
	;;
	hp*|HP* )
	    F77LIBS="-lU77 -lcl -lm"
	    F77CONV="-DUSE_LOWER"
	    C_SH_FLAGS="+z"
	    F_SH_FLAGS="+z"
	    LD_SH_FLAGS=""
	    SHLIB_BUILD="ld -b -o"
	    DL="YES"
            DL_HP="YES"
	;;
	irix*|IRIX*|Irix*|sgi*|SGI* )
            F77LIBS="-lF77 -lI77 -lU77 -lisam -lfpe"
            F77CONV="-DUSE_F2C"
	;;
	linux*|LINUX*|Linux* )
	    CC="gcc"
	    #CFLAGS="-O2 $CFLAGSD"
	    FFLAGS="-O2"
            F77CONV="-DUSE_F2C"
	;;
	next*|NeXT*|NEXT* )
	    CC="gcc"
            CFLAGS="-O"
            FFLAGS="-O"
            F77CONV="-DUSE_F2C"
	;;
	SunOS-* )
	    case "$system" in
		*5.* )
		   F77LIBS="-L/opt/SUNWspro/SC4.0/lib -lF77 -lM77 -lsunmath"
		   F77CONV="-DUSE_F2C"
		   C_SH_FLAGS="-K PIC"
		   F_SH_FLAGS="-KPIC"
		   LD_SH_FLAGS="-G"
                   CFLAGS="-O"
                   FFLAGS="-O"
		   ;;
		*4* )
		   F77LIBS="-L/usr/lang/SC1.0 -lF77"
		   F77CONV="-DUSE_F2C"
		   C_SH_FLAGS="-pic"
		   F_SH_FLAGS="-pic"
		   LD_SH_FLAGS=""
                   CFLAGS="-O"
                   FFLAGS="-O"
		   ;;
		esac
	;;
	sx*|Sx*|SX*|monte*|Monte*|MONTE* )
	;;
	ultrix*|ULTRIX* )
	    F77LIBS="-L. -lF77 -lI77 -lUfor -lfor -lutil -lots \
		     -L/usr/lib/cmplrs/fort -li_31"
	    F77CONV=-DUSE_F2C
            CFLAGS="-O"
            FFLAGS="-O"
	;;
	*4* )   # System V Release 4.0
	   F77LIBS="?"
	   F77CONV="-DUSE_F2C"
	   C_SH_FLAGS="-K PIC"
	   F_SH_FLAGS="-KPIC"
	   LD_SH_FLAGS="-G"
           CFLAGS="-O"
           FFLAGS="-O"
    esac

# echo "And the system is ${system}!"
#CFLAGS=${CFLAGS} ${CFLAGSD}
echo "compiler flags are: $CFLAGS"



#
# If nothing specified, leave it up to configure
#
if test -z "$CC" ; then
  # Extract the first word of "gcc", so it can be a program name with args.
set dummy gcc; ac_word=$2
echo $ac_n "checking for $ac_word""... $ac_c" 1>&6
echo "configure:1177: checking for $ac_word" >&5
if eval "test \"`echo '$''{'ac_cv_prog_CC'+set}'`\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  if test -n "$CC"; then
  ac_cv_prog_CC="$CC" # Let the user override the test.
else
  IFS="${IFS= 	}"; ac_save_ifs="$IFS"; IFS=":"
  ac_dummy="$PATH"
  for ac_dir in $ac_dummy; do
    test -z "$ac_dir" && ac_dir=.
    if test -f $ac_dir/$ac_word; then
      ac_cv_prog_CC="gcc"
      break
    fi
  done
  IFS="$ac_save_ifs"
fi
fi
CC="$ac_cv_prog_CC"
if test -n "$CC"; then
  echo "$ac_t""$CC" 1>&6
else
  echo "$ac_t""no" 1>&6
fi

if test -z "$CC"; then
  # Extract the first word of "cc", so it can be a program name with args.
set dummy cc; ac_word=$2
echo $ac_n "checking for $ac_word""... $ac_c" 1>&6
echo "configure:1207: checking for $ac_word" >&5
if eval "test \"`echo '$''{'ac_cv_prog_CC'+set}'`\" = set"; then
  echo $ac_n "(cached) $ac_c" 1>&6
else
  if test -n "$CC"; then
  ac_cv_prog_CC="$CC" # Let the user override the test.
else
  IFS="${IFS= 	}"; ac_save_ifs="$IFS"; IFS=":"
  ac_prog_rejected=no
  ac_dummy="$PATH"
  for ac_dir in $ac_dummy; do
    test -z "$ac_dir" && ac_dir=.
    if test -f $ac_dir/$ac_word; then
      if test "$ac_dir/$ac_word" = "/usr/ucb/cc"; then
        ac_prog_rejected=yes
	continue
      fi
      ac_cv_prog_CC="cc"
      break
    fi
  done
  IFS="$ac_save_ifs"
if test $ac_prog_rejected = yes; then
  # We found a bogon in the path, so make sure we never use it.
  set dummy $ac_cv_prog_CC
  shift
  if test $# -gt 0; then
    # We chose a different compiler from the bogus one.
    # However, it has the same basename, so the bogon will be chosen
    # first if we set CC to just the basename; use the full file name.
    shift
    set dummy "$ac_dir/$ac_word" "$@"
    shift
    ac_cv_prog_CC="$@"
  fi
fi
fi
fi
CC="$ac_cv_prog_CC"
if test -n "$CC"; then
  echo "$ac_t""$CC" 1>&6
else
  echo "$ac_t""no" 1>&6
fi

  if test -z "$CC"; then
    case "`uname -s`" in
    *win32* | *WIN32*)
      # Extract the first word of "cl", so it can be a program name with args.
